{
    "manifest_version": 3,
    "name": "Git Repo Bookmark Manager",
    "version": "1.1.1",
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://github.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}
